{
  "expo": {
    "name": "StockFellow",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./apps/mobile-app/assets/stockfellow_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "mobile-app",
    "platforms": [
      "ios",
      "android"
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "ios": {
      "supportsTablet": true,
      "runtimeVersion": {
        "policy": "appVersion"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./apps/mobile-app/assets/icons/savings.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.ltshikila.mobileapp",
      "runtimeVersion": "1.0.0"
    },
    "web": {
      "favicon": "./apps/mobile-app/assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./apps/mobile-app/assets/stockfellow_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "imageWidth": 250
        }
      ]
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e462114e-a621-4fc6-9dda-5bc774753635"
      }
    }
  }
}
