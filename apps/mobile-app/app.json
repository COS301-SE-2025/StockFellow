{
  "expo": {
    "name": "StockFellow",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/stockfellow_logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "mobile-app",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/stockfellow_logo.png",
        "backgroundColor": "#00000000"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/icons/savings.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-splash-screen",
        {
          "image": "./assets/stockfellow_logo.png",
          "resizeMode": "contain",
          "backgroundColor": "#00000000",
          "imageWidth": 200
        }
      ]
    ]
  }
}